<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>GraphQLServletFilter</filter-name>
  	<display-name>GraphQLServletFilter</display-name>
  	<description></description>
  	<filter-class>fivium.rns.servlets.GraphQLServletFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>GraphQLServletFilter</filter-name>
  	<url-pattern>*</url-pattern>
  </filter-mapping>
	<servlet>
		<servlet-name>DoLogin</servlet-name>
		<display-name>DoLogin</display-name>
		<description></description>
		<servlet-class>fivium.rns.servlets.DoLogin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DoSignUp</servlet-name>
		<display-name>DoSignUp</display-name>
		<description></description>
		<servlet-class>fivium.rns.servlets.DoSignUp</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DoRetrievePatientDetails</servlet-name>
		<display-name>DoRetrievePatientDetails</display-name>
		<description></description>
		<servlet-class>fivium.rns.servlets.DoRetrievePatientDetails</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DoRetrievePatient</servlet-name>
		<display-name>DoRetrievePatient</display-name>
		<description></description>
		<servlet-class>fivium.rns.servlets.DoRetrievePatient</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DoRetrieveActivityData</servlet-name>
		<display-name>DoRetrieveActivityData</display-name>
		<description></description>
		<servlet-class>fivium.rns.servlets.DoRetrieveActivityData</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DoGetPatientSurvey</servlet-name>
		<display-name>DoGetPatientSurvey</display-name>
		<description></description>
		<servlet-class>fivium.rns.servlets.DoGetPatientSurvey</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DoLogin</servlet-name>
		<url-pattern>/DoLogin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DoSignUp</servlet-name>
		<url-pattern>/DoSignUp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DoRetrievePatientDetails</servlet-name>
		<url-pattern>/DoRetrievePatientDetails</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DoRetrievePatient</servlet-name>
		<url-pattern>/DoRetrievePatient</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DoRetrieveActivityData</servlet-name>
		<url-pattern>/DoRetrieveActivityData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DoGetPatientSurvey</servlet-name>
		<url-pattern>/DoGetPatientSurvey</url-pattern>
	</servlet-mapping>	
</web-app>
